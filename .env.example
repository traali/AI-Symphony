# API Keys
OPENROUTER_API_KEY=your_openrouter_key_here
GITHUB_PAT=ghp_your_github_pat_here

# Repository Configuration
# The full HTTPS URL to an EXISTING, EMPTY repository on your GitHub.
REPO_URL=https://github.com/YOUR_USERNAME/YOUR_TARGET_REPO.git

# Optional Configuration
# DEBUG_MODE: Keep workspace files for inspection (true/false, default: false)
DEBUG_MODE=false

# MAX_RETRIES: Number of retry attempts for GitHub API calls (default: 3)
MAX_RETRIES=3

# LOG_LEVEL: Logging verbosity (DEBUG, INFO, WARNING, ERROR, default: INFO)
LOG_LEVEL=INFO
